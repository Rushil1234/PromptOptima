# Database (optional - for persistence)
MONGODB_URI=mongodb://localhost:27017/optimizer

# Security
API_SECRET_KEY=your_secret_key_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
